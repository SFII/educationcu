{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "React": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "global-strict": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "no-console": 0,
    "no-underscore-dangle": 0
  },
  "plugins": [
    "react"
  ]
}
